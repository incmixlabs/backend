{
  "name": "incmix-api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean",
    "prebuild": "pnpm clean",
    "dev": "turbo dev --parallel --ui tui",
    "lint": "biome check .",
    "lint:fix": "biome check --fix --unsafe",
    "lint:packages": "pnpx sherif@latest",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "test:e2e": "turbo test:e2e"
  },
  "engines": {
    "node": ">=22.13.0",
    "pnpm": ">=9"
  },
  "pnpm": {
    "neverBuiltDependencies": [
      "@incmix/utils"
    ]
  },
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@faker-js/faker": "9.8.0",
    "@turbo/gen": "2.5.4",
    "concurrently": "^9.2.0",
    "lefthook": "1.10.4",
    "tsc-alias": "catalog:",
    "turbo": "2.5.4"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "core-js-pure"
  ]
}
