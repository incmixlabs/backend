{
  "name": "incmix-api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean",
    "prebuild": "pnpm clean",
    "dev": "turbo dev --parallel --ui tui",
    "lint": "NODE_NO_WARNINGS=1 biome check .",
    "lint:fix": "NODE_NO_WARNINGS=1 biome check --fix --unsafe",
    "lint:packages": "pnpx sherif@latest",
    "typecheck": "turbo typecheck",
    "pretest": "docker compose -f docker-compose-test.yml up -d --wait",
    "posttest": "docker compose -f docker-compose-test.yml down --remove-orphans",
    "test": "turbo test --ui tui",
    "test:e2e": "turbo test:e2e"
  },
  "engines": {
    "node": ">=22.13.0",
    "pnpm": ">=9"
  },
  "pnpm": {
    "neverBuiltDependencies": [
      "@incmix/utils"
    ],
    "overrides": {
      "brace-expansion@>=1.0.0 <=1.1.11": "1.1.12",
      "brace-expansion@>=2.0.0 <=2.0.1": "2.0.2",
      "form-data@>=4.0.0 <4.0.4": "4.0.4",
      "vite@>=6.0.0 <=6.3.5": "6.3.6",
      "tmp@<=0.2.3": "0.2.4",
      "glob@<=7.2.3": "10.4.5",
      "inflight@<=1.0.6": "^1.0.6",
      "rimraf@<=3.0.2": "5.0.7"
    },
    "ignoredBuiltDependencies": [
      "@incmix/utils"
    ]
  },
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@faker-js/faker": "10.0.0",
    "@turbo/gen": "2.5.6",
    "concurrently": "^9.2.1",
    "lefthook": "1.12.3",
    "tsc-alias": "catalog:",
    "turbo": "2.5.6",
    "vite-tsconfig-paths": "catalog:"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "core-js-pure"
  ]
}
